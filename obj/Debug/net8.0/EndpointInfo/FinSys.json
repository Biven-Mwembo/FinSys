    "/api/Transactions": {
      "post": {
        "tags": [
          "Transactions"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "Amount",
                  "Channel",
                  "Currency",
                  "Date"
                ],
                "type": "object",
                "properties": {
                  "Date": {
                    "type": "string",
                    "format": "date-time"
                  },
                  "Amount": {
                    "minimum": 0.01,
                    "type": "number",
                    "format": "double"
                  },
                  "Currency": {
                    "type": "string"
                  },
                  "Channel": {
                    "type": "string"
                  },
                  "Motif": {
                    "type": "string"
                  },
                  "File": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              },
              "encoding": {
                "Date": {
                  "style": "form"
                },
                "Amount": {
                  "style": "form"
                },
                "Currency": {
                  "style": "form"
                },
                "Channel": {
                  "style": "form"
                },
                "Motif": {
                  "style": "form"
                },
                "File": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      },
      "get": {
        "tags": [
          "Transactions"
        ],
